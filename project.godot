; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="CarRacing"
run/main_scene="res://scenes/game.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Easytech"
boot_splash/image="res://assets/images/splash_screen.png"
config/icon="res://icon.png"

[display]

window/size/width=540
window/size/height=960
window/size/fullscreen=true
window/size/test_width=320
window/size/test_height=480
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep_height"
window/stretch/shrink="1"

[rendering]

environment/default_environment="res://default_env.tres"
